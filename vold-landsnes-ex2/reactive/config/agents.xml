<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
	<!-- A list of agents and their behavior classes -->

  <agent name="reactive-array">
    <set class-path="bin/"/>
    <set class-name="template.ReactiveArray"/>
    <set discount-factor="0.9"/>
  </agent>
  
  <agent name="reactive-rla">
    <set class-path="bin/"/>
    <set class-name="template.ReactiveAgent"/>
    <set discount-factor="0.9"/>
  </agent>
	
	<!-- #### REACTIVE AGENTS ### -->
	<agent name="reactive-random">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveTemplate"/>
		<set discount-factor=".85"/>
	</agent>
	
</agents>
